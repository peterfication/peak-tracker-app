{
  "name": "PeakTracker",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "android": "echo 'Does not work with dotenv, see https://github.com/goatandsheep/react-native-dotenv/issues/393#issuecomment-1380913100'; exit; APP_ENV=development react-native run-android",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "graphql:codegen": "graphql-codegen --config graphql-codegen.yml",
    "graphql:validate": "graphql-inspector validate 'src/graphql/**/*.graphql' 'src/graphql/schema.json' --deprecated",
    "ios": "echo 'Does not work with dotenv, see https://github.com/goatandsheep/react-native-dotenv/issues/393#issuecomment-1380913100'; exit; APP_ENV=development react-native run-ios --simulator='iPhone 14'",
    "lint": "eslint .",
    "spellcheck": "cspell '**'",
    "spellcheck:list": "cspell '**' --words-only --unique --no-progress",
    "start": "APP_ENV=development react-native start",
    "storybook:generate": "sb-rn-get-stories",
    "storybook:start": "APP_ENV=storybook react-native start",
    "storybook:watch": "sb-rn-watcher",
    "test": "jest",
    "test:clear": "jest --clearCache",
    "test:coverage": "jest --verbose --coverage",
    "test:coverage:open": "jest --verbose --coverage; open ./coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.7.12",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "graphql": "^16.6.0",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-app-auth": "^v7.0.0-rc2",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-reanimated": "^3.0.2",
    "react-native-screens": "^3.20.0",
    "react-native-ui-lib": "^7.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-inspector/cli": "^3.4.9",
    "@graphql-inspector/validate-command": "^3.4.7",
    "@prettier/plugin-xml": "^2.2.0",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/datetimepicker": "^7.0.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-community/slider": "^4.4.2",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-controls": "^7.0.11",
    "@storybook/addon-ondevice-actions": "6.5.0-rc.0",
    "@storybook/addon-ondevice-controls": "6.5.0-rc.0",
    "@storybook/react-native": "6.5.0-rc.11",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.0.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.34",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.57.1",
    "babel-jest": "^29.2.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "cspell": "^6.31.1",
    "eslint": "^8.37.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-testing-library": "^5.10.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.0",
    "prettier": "^2.8.7",
    "react-dom": "18.2.0",
    "react-native-dotenv": "^3.4.8",
    "react-native-safe-area-context": "^4.5.0",
