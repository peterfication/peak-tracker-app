{
  "name": "PeakTracker",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "android": "echo 'Does not work with dotenv, see https://github.com/goatandsheep/react-native-dotenv/issues/393#issuecomment-1380913100'; exit; APP_ENV=development react-native run-android",
    "build:android": "mkdir -p android/build && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/build/index.android.bundle --assets-dest android/build",
    "build:ios": "mkdir -p ios/build && react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/build/index.ios.bundle --assets-dest ios/build",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "graphql:codegen": "graphql-codegen --config graphql-codegen.yml",
    "graphql:validate": "graphql-inspector validate 'src/graphql/**/*.graphql' 'src/graphql/schema.json' --deprecated",
    "ios": "echo 'Does not work with dotenv, see https://github.com/goatandsheep/react-native-dotenv/issues/393#issuecomment-1380913100'; exit; APP_ENV=development react-native run-ios --simulator='iPhone 14'",
    "lint": "eslint .",
    "spellcheck": "cspell '**'",
    "spellcheck:list": "cspell '**' --words-only --unique --no-progress",
    "start": "APP_ENV=development react-native start",
    "storybook:generate": "sb-rn-get-stories",
    "storybook:watch": "sb-rn-watcher",
    "test": "jest",
    "test:clear": "jest --clearCache",
    "test:coverage": "jest --verbose --coverage",
    "test:coverage:open": "jest --verbose --coverage; open ./coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/datetimepicker": "^7.6.1",
    "@react-native-community/slider": "^4.4.3",
    "@react-native/eslint-config": "^0.74.0",
    "@react-native/metro-config": "^0.74.0",
    "@react-navigation/bottom-tabs": "^6.5.9",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "graphql": "^16.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-app-auth": "^7.1.0",
    "react-native-dotenv": "^3.4.9",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.13.2",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-ui-lib": "^7.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@graphql-inspector/cli": "^4.0.2",
    "@graphql-inspector/validate-command": "^4.0.2",
    "@prettier/plugin-xml": "^3.2.1",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-ondevice-actions": "^6.5.6",
    "@storybook/addon-ondevice-controls": "^6.5.6",
    "@storybook/react-native": "^6.5.6",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.3.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.28",
    "@types/react-test-renderer": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "cspell": "^7.3.8",
    "eslint": "^8.51.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-testing-library": "^6.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "@babel/core": "^7.20.2",
    "babel-loader": "^8.3.0"
  },
  "volta": {
    "node": "18.18.0"
  },
  "engines": {
    "node": "18.18.0"
  },
  "packageManager": "yarn@3.6.4"
}
